generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id               Int                @id @default(autoincrement())
  name             String
  email            String
  company          String
  password         String
  UserOfferLibrary UserOfferLibrary[]
}

model UserOfferLibrary {
  offer_id String
  user_id  Int
  User     User   @relation(fields: [user_id], references: [id])

  @@id([offer_id, user_id])
}
